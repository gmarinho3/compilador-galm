let a = 1;

assertEquals(a, 1);

let b = a = a + 1;

assertEquals(a, 2);
assertEquals(b, 2);

let c = a = b = 1;

assertEquals(c, 1);
assertEquals(a, 1);
assertEquals(b, 1);

let d = a = b = c = 3;

assertEquals(d, 3);
assertEquals(a, 3);
assertEquals(b, 3);
assertEquals(c, 3);

let e = a = b = c = 5;

assertEquals(e, 5);
assertEquals(a, 5);
assertEquals(b, 5);
assertEquals(c, 5);